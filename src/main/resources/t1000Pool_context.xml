<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id = "asd" class="ru.javabegin.training.spring.impls.pool.ColorBlue">
        <qualifier type="ru.javabegin.training.spring.interfaces.T1000PoolQualifier">
            <attribute key="color" value="BLUE"/>
            <attribute key="colorname" value="blue"/>
        </qualifier>
    </bean>

    <bean class="ru.javabegin.training.spring.impls.pool.T1000Pool">
        <qualifier type="ru.javabegin.training.spring.interfaces.T1000PoolQualifier">
            <attribute key="color" value="GREEN"/>
            <attribute key="colorname" value="green"/>
        </qualifier>
    </bean>

    <bean class="ru.javabegin.training.spring.impls.pool.T1000Pool">
        <qualifier type="ru.javabegin.training.spring.interfaces.T1000PoolQualifier">
            <attribute key="color" value="RED"/>
            <attribute key="colorname" value="red"/>
        </qualifier>
    </bean>




</beans>