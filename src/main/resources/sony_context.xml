<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd" xmlns:c="http://www.springframework.org/schema/c"
       default-lazy-init="true"
>


    <bean id="sonyHand" class="ru.javabegin.training.spring.impls.sony.SonyHand"/>
    <bean id="sonyHead1" class="ru.javabegin.training.spring.impls.sony.SonyHead">
        <property name="color" value="Golden"></property>
        <qualifier value="sonyGoldenHead"></qualifier>
    </bean>

    <bean id="sonyHead2" class="ru.javabegin.training.spring.impls.sony.SonyHead">
        <property name="color" value="Bronze"></property>

        <qualifier value="sonyBronzeHead"></qualifier>
    </bean>

    <bean id="sonyLeg" class="ru.javabegin.training.spring.impls.sony.SonyLeg" />


</beans>